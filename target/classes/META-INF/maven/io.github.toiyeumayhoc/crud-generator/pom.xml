<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://maven.apache.org/POM/4.0.0"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>crud-generator</artifactId>
	<name>CRUD generator for Spring Web Project</name>
	<version>1.0.1</version>
	<groupId>io.github.toiyeumayhoc</groupId>
	<url>http://github.com/toiyeumayhoc/crud-generator</url>
	<description>A simple library for auto generate CRUD controller for Spring Web application</description>
	<parent>
		<groupId>org.springframework.boot</groupId>
		<artifactId>spring-boot-starter-parent</artifactId>
		<version>2.5.6</version>
		<relativePath /> <!-- lookup parent from repository -->
	</parent>

	<licenses>
		<license>
			<name>MIT License</name>
			<url>http://www.opensource.org/licenses/mit-license.php</url>
		</license>
	</licenses>

	<developers>
		<developer>
			<name>Cuong Le</name>
			<email>cuonglv1109@gmail.com</email>
			<organization>com.github.toiyeumayhoc</organization>
			<organizationUrl>http://github.com/toiyeumayhoc/crud-generator</organizationUrl>
		</developer>
	</developers>

	<scm>
		<connection>scm:git:git://github.com/toiyeumayhoc/crud-generator.git</connection>
		<developerConnection>scm:git:ssh://github.com:toiyeumayhoc/crud-generator.git</developerConnection>
		<url>http://github.com/toiyeumayhoc/crud-generator</url>
	</scm>

	<dependencies>
		<dependency>
			<groupId>com.google.auto.service</groupId>
			<artifactId>auto-service</artifactId>
			<version>${auto-service.version}</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>org.projectlombok</groupId>
			<artifactId>lombok</artifactId>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>org.apache.commons</groupId>
			<artifactId>commons-lang3</artifactId>
		</dependency>

	</dependencies>

	<properties>
		<auto-service.version>1.0-rc2</auto-service.version>
	</properties>

</project>